{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "warn",
        "no-tabs": "warn",
        "no-mixed-spaces-and-tabs": "warn",
        "arrow-parens": "warn",
        "semi": "off"
    }
}